@enum
class Shape:
    # Definition using AnnAssign
    # VariantName: [Types]
    Circle: [f64]
    Rectangle: [f64, f64]
    Point: []

class User:
    id: i32
    name: String

def main():
    # Constructor mapping: Shape.Circle(...)
    # We need to support this.
    s: Shape = Shape.Circle(10.0)
    
    match s:
        case Shape.Circle(r):
            print(r)
        case Shape.Rectangle(w, h):
            print(w * h)
        case Shape.Point:
            print("Point")
            
    u: User = User(id=1, name="Alice")
    
    # Struct destructuring
    match u:
        case User(id=1, name=n):
            print(n)
        case _:
            print("Other")
