def main():
    # Basic Option
    x: Option[i32] = Some(10)
    y: Option[i32] = None

    print(x)
    print(y)

    # Check unwrap
    if x.is_some():
        print(x.unwrap())
    
    # Result
    res: Result[i32, String] = Ok(42)
    err: Result[i32, String] = Err("Something failed")
    print(res)
    print(err)
